    }
    @if("Agenda".Equals(Model.NomeView))
    {
        <link rel="stylesheet" type="text/css" href="~/css/agenda.css">
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
        <link src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js">
        <link src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.bundle.min.js">
        <link src="https://code.jquery.com/jquery-3.3.1.min.js">
    }
    @if("Logadissimo".Equals(Model.NomeView))
    {
        <link rel="stylesheet" type="text/css" href="~/css/nav2.css">
    }
    @if("Dashboard".Equals(@Model.NomeView))
    {
       
        <link href="~/css/dashboard.css" rel="stylesheet" type="text/css">
    }

    


</head>


    <header >
    @if ("Nav1".Equals(Model.NomeView2))
    {
        Html.RenderPartial("_NavPrincipal");
    }

    @if("Nav2".Equals(Model.NomeView2))
    {
        Html.RenderPartial("_Nav2");
    }

    </header>

@RenderBody()

</html>